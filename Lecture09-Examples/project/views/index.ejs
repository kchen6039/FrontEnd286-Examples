<%- include('header') %>



    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h1>
                        <%= title %>
                    </h1>
                </div>
                <% for( var index = 0; index < notes.length; index++){ %>
                    <%  var note = notes[index]; %>
                        <div class="col-md-4">
                            <div class="thumbnail">
                                <img src="https://unsplash.it/400/300?image=10<%= index %>" alt="">
                                <div class="caption">
                                    <h3>
                                        <a href="/note?id=<%= index %>">
                                            <%= note.title %>
                                        </a>

                                    </h3>
                                    <p>
                                        <%= note.text %>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <% if( index % 3 === 2) { %>
                            <div class="clearfix"></div>
                            <% } %>

                                <% } %>

            </div>
        </div>
    </section>


    <%- include('footer') %>